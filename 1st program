x = 5
print(type(x))

x = str("Hello World")
print(type(x))

x = int(20)
print(type(x))

x = float(20.5)
print(type(x))

x = complex(1j)
print(type(x))

x = list(("apple", "banana", "cherry"))
print(type(x))

x = dict(name="John", age=36)
print(type(x))

x = tuple(("apple", "banana", "cherry"))
print(type(x))
